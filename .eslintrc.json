{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "react/jsx-indent": "off",
        "linebreak-style": 0,
        "indent": "off",
        "no-trailing-spaces": [2, { "skipBlankLines": true }]
    },
    "env": {
        "browser": true
    }
}